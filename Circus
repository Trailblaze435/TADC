class Circus:
  
  def __init__(self, name, lastname):
    self.name = name
    self.lastname = lastname
    
    def get_name(self):
        return self.name
    
    def get_lastname(self):
        return self.lastname
    
p = Circus("Pomni", "Jester")
print("Hello, My name is" + p.name + "." "The" + p.lastname + "of the digital circus")
